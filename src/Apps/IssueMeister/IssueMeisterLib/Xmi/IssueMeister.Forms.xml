<?xml version="1.0" encoding="utf-8" ?>
<item>
    <item p1:type="dm:///_internal/types/internal#DatenMeister.Models.DefaultTypes.Package"
          name="IssueMeister" xmlns:p1="http://www.omg.org/spec/XMI/20131001">

        <!-- The filter -->
        <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.DefaultTypes.Package"
                         name="DataView">
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.DefaultTypes.Package"
                             name="Filter Open">
                <packagedElement
                        p1:type="dm:///_internal/types/internal#DatenMeister.Models.DataViews.FilterPropertyNode"
                        name="Open Issues"
                        input-ref="#IssueMeister.FilterOpen.Node1" property="state" value="Open" comparisonMode="Equal"
                        p1:id="IssueMeister.OpenIssuesViews"/>
                <packagedElement
                        p1:type="dm:///_internal/types/internal#DatenMeister.Models.DataViews.DynamicSourceNode"
                        name="input" p1:id="IssueMeister.FilterOpen.Node1"/>
            </packagedElement>
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.DefaultTypes.Package"
                             name="Filter In Work">
                <packagedElement
                        p1:type="dm:///_internal/types/internal#DatenMeister.Models.DataViews.FilterPropertyNode"
                        name="In Work Issues"
                        input-ref="#IssueMeister.FilterInWork.Node1" property="state" value="InWork" comparisonMode="Equal"
                        p1:id="IssueMeister.InWorkIssuesViews"/>
                <packagedElement
                        p1:type="dm:///_internal/types/internal#DatenMeister.Models.DataViews.DynamicSourceNode"
                        name="input" p1:id="IssueMeister.FilterInWork.Node1"/>
            </packagedElement>
        </packagedElement>

        <!-- The forms -->
        <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.DefaultTypes.Package"
                         p1:id="Package-Issue" name="Forms">
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.DetailForm"
                             p1:id="IssueFormDetail" name="Issue - Detail" hideMetaInformation="True">
                <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="id"
                       title="id" width="10"/>
                <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="name"
                       title="name"/>
                <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.DropDownFieldData" name="state"
                       title="state" valuesByEnumeration-ref="dm:///_internal/types/internal#IssueMeister.IssueState"/>
                <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData"
                       name="description" title="description" lineHeight="8"/>
                <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.MetaClassElementFieldData"
                       name="Metaclass"/>
            </packagedElement>
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.ExtentForm"
                             p1:id="IssueFormList" name="Issue - List">
                <tab p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.DetailForm" name="Detail">
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="id"
                           title="id" isReadOnly="True" width="10"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="name"
                           title="name" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="state"
                           title="state" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData"
                           name="description" title="description" lineHeight="8"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.MetaClassElementFieldData"
                           name="Metaclass"/>
                </tab>
            </packagedElement>
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.ExtentForm"
                             p1:id="IssuePackageList" name="Packages - List">
                <tab p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.ListForm" name="Issues"
                     metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue" property="packagedElement">
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="id"
                           title="id" isReadOnly="True" width="10" p1:id="IssuePackageList.Issues.id"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="name"
                           title="name" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="state"
                           title="state" isReadOnly="True" p1:id="IssuePackageList.Issues.state"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData"
                           name="description" title="description" lineHeight="8"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.MetaClassElementFieldData"
                           name="Metaclass"/>
                    <defaultTypesForNewElements name="Issue"
                                                metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue"
                                                parentProperty="packagedElement"
                                                p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.DefaultTypeForNewElement"/>
                    <sortingOrder
                            field-ref="#IssuePackageList.Issues.state" isDescending="true" />
                    <sortingOrder
                            field-ref="#IssuePackageList.Issues.id" />
                </tab>
                <tab p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.ListForm" name="Open Issues"
                     metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue" property="packagedElement"
                     viewNode-ref="#IssueMeister.OpenIssuesViews">
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="id"
                           title="id" isReadOnly="True" width="10"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="name"
                           title="name" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="state"
                           title="state" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData"
                           name="description" title="description" lineHeight="8"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.MetaClassElementFieldData"
                           name="Metaclass"/>
                    <defaultTypesForNewElements name="Issue"
                                                metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue"
                                                parentProperty="packagedElement"
                                                p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.DefaultTypeForNewElement"/>
                </tab>
                <tab p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.ListForm" name="In Work Issues"
                     metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue" property="packagedElement"
                     viewNode-ref="#IssueMeister.InWorkIssuesViews">
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="id"
                           title="id" isReadOnly="True" width="10"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="name"
                           title="name" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData" name="state"
                           title="state" isReadOnly="True"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.TextFieldData"
                           name="description" title="description" lineHeight="8"/>
                    <field p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.MetaClassElementFieldData"
                           name="Metaclass"/>
                    <defaultTypesForNewElements name="Issue"
                                                metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue"
                                                parentProperty="packagedElement"
                                                p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.DefaultTypeForNewElement"/>
                </tab>
            </packagedElement>

            <!-- The Form Associations -->
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.FormAssociation"
                             p1:id="IssueAssociationDetail" formType="Detail"
                             form-ref="#IssueFormDetail"
                             metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue" name="Issue - Detail - Association"/>

            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.FormAssociation"
                             p1:id="IssueAssociationList" formType="TreeItemDetail"
                             form-ref="#IssueFormList" metaClass-ref="dm:///_internal/types/internal#IssueMeister.Issue"
                             name="Issue - Tree - Association"/>

            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.FormAssociation"
                             p1:id="IssuePackageAssociationList" formType="TreeItemDetail"
                             form-ref="#IssuePackageList"
                             metaClass-ref="dm:///_internal/types/internal#DatenMeister.Models.DefaultTypes.Package"
                             name="Packages in IssueMeister"
                             viewModeId="IssueMeister"/>

            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.FormAssociation"
                             p1:id="IssueExtentAssociationList" formType="TreeItemExtent"
                             form-ref="#IssuePackageList"
                             extentType="IssueMeister"
                             name="Packages in IssueMeister"
                             viewModeId="IssueMeister"/>

            <!-- Declares the view mode -->
            <packagedElement p1:type="dm:///_internal/types/internal#DatenMeister.Models.Forms.ViewModes.ViewMode"
                             name="ViewMode for IssueMeister"
                             id="IssueMeister" defaultExtentType="IssueMeister"/>
        </packagedElement>
    </item>
</item>