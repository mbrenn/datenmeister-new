<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>8</LangVersion>
    <OutputType>WinExe</OutputType>
    <Nullable>disable</Nullable>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
    <Configurations>Debug;Release;WPF</Configurations>
    <Platforms>AnyCPU</Platforms>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
  </PropertyGroup>  
  <PropertyGroup>
    <ApplicationIcon>StundenMeister.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="StundenMeister.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DatenMeister">
      <Version>1.1.1</Version>
    </PackageReference>
    <PackageReference Include="DatenMeister.WPF">
      <Version>1.1.1</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.CSharp">
      <Version>4.7.0</Version>
    </PackageReference>
    <PackageReference Include="System.Reflection.TypeExtensions">
      <Version>4.7.0</Version>
    </PackageReference>
    <PackageReference Include="System.ValueTuple">
      <Version>4.5.0</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="Xmi\Elements.xml" />
    <EmbeddedResource Include="Xmi\Elements.xml" />
  </ItemGroup>
</Project>