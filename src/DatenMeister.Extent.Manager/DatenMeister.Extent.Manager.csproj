      <ProjectReference Include="..\DatenMeister.Validators\DatenMeister.Validators.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Autofac" Version="6.5.0" />
    </ItemGroup>

</Project>
