{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es3",
    "module": "amd",
    "allowJs": true,
    "types": [ "jquery", "underscore", "requirejs" ]
  },
  "src": [
    "htdocs/js/*.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot",
    "bin",
    "**/*.js"
  ]
}

